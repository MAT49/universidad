<h1>Listing ints</h1>

<table>
  <thead>
    <tr>
      <th>Num</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ints.each do |int| %>
      <tr>
        <td><%= int.num %></td>
        <td><%= link_to 'Show', int %></td>
        <td><%= link_to 'Edit', edit_int_path(int) %></td>
        <td><%= link_to 'Destroy', int, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Int', new_int_path %>
